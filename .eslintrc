{
  "plugins" : ["vitest"],
  "env": {
    "vitest/globals": true
  },
  "extends" : ["eslint:recommended"],
  "rules": {
    "no-console":0,
    "no-process-exit":0,
    "no-fallthrough":0
  },
  "overrides": [{
    "files": [ "__tests__/*.js" ],
    "rules": {
      "no-unused-vars":0
    }
  }]
}
